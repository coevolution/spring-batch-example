<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       default-lazy-init="false">
    <dubbo:consumer check="false"/>
    <!--消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
    <dubbo:application name="tp-payment-batch" owner="luxiaopeng"/>
    <!--zookeeper注册中心 -->
    <dubbo:registry protocol="zookeeper" address="dubbo-zk.dashu.ds:2181"/>
    <!--<dubbo:registry protocol="zookeeper" address="${dubbo.zkaddress}"/>-->
    <!--使用multicast广播注册中心暴露的服务地址 -->
    <!--<dubbo:registryaddress="multicast://10.57.41.19:1234" /> -->
    <!-- 用dubbo协议在8080端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="9191"/>
    <dubbo:provider timeout="10000"/>
    <!-- 生成远程服务代理，可以和本地bean一样使用demoService-->
    <dubbo:reference id="mailFacade" interface="com.treefintech.notify.facade.service.MailFacade"/>

    <dubbo:annotation package="com.treefinance.payment"/>
</beans>
